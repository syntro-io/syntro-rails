      $('#flash_alert').show();
    }
 <% end %>
 <% if schedule.wednesday? %>
   $('#w<%= time_as_string(schedule.start_time, "") %>').append('<div class="scheduledItem">&nbsp; <%= time_as_string(schedule.start_time, ":") %>: <%= link_to truncate(schedule.test_plan.name, :length => 13), schedule, :title => schedule.test_plan.name %></div>');
