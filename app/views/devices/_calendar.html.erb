 <% end %>
 <% if schedule.saturday? %>
   $('#s<%= time_as_string(schedule.start_time, "") %>').append('<div class="scheduledItem">&nbsp; <%= time_as_string(schedule.start_time, ":") %>: <%= link_to truncate(schedule.test_plan.name, :length => 13), schedule, :title => schedule.test_plan.name %></div>');
   if ($('#s<%= time_as_string(schedule.start_time, "") %>').children('.scheduledItem').size() > 3) {
       $('#flash_alert').show();
     }
 <% end %>
 <% if schedule.sunday? %>
   $('#u<%= time_as_string(schedule.start_time, "") %>').append('<div class="scheduledItem">&nbsp; <%= time_as_string(schedule.start_time, ":") %>: <%= link_to truncate(schedule.test_plan.name, :length => 13), schedule, :title => schedule.test_plan.name %></div>');
   if ($('#u<%= time_as_string(schedule.start_time, "") %>').children('.scheduledItem').size() > 3) {
       $('#flash_alert').show();
     }
 <% end %>

<% end %>
