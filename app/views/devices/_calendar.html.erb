<script>
<% @schedules.each do |schedule| %> 
 <% if schedule.monday? %>
   $('#m<%= time_as_string(schedule.start_time, "") %>').append('<div class="scheduledItem">&nbsp; <%= time_as_string(schedule.start_time, ":") %>: <%= link_to truncate(schedule.test_plan.name, :length => 13), schedule, :title => schedule.test_plan.name %></div>');
   if ($('#m<%= time_as_string(schedule.start_time, "") %>').children('.scheduledItem').size() > 3) {
     $('#flash_alert').show();
   } 
 <% end %>
 <% if schedule.tuesday? %>
   $('#t<%= time_as_string(schedule.start_time, "") %>').append('<div class="scheduledItem">&nbsp; <%= time_as_string(schedule.start_time, ":") %>: <%= link_to truncate(schedule.test_plan.name, :length => 13), schedule, :title => schedule.test_plan.name %></div>');
